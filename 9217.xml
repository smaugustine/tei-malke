    <body>

      
      <listRelation>
        <relation name="subjects">
          <desc>
            <listPerson>
              
              
              <person xml:id="julitta">
                <persName xml:lang="gez">ኢየሉጣ፡</persName>
                <persName xml:lang="gez" type="normalized">ʾIyaluṭṭā</persName>
                <persName xml:lang="en">Julitta</persName>
